@model Lib.ViewModels.CreateAssessmentVM
@{
    ViewBag.Title = "ViewClientAssessment";
}

<h2>ViewClientAssessment</h2>

@Html.BeginForm("ViewClientAssessment", "Assessment", FormMethod.Post)
@Html.Label("Start Date", new { @class = "LableFormat" })
@Html.TextBox("StartDate", DateTime.Now, htmlAttributes: new { @class = "form-control TextBoxFormat" })
@Html.Label("End Date", new { @class = "LableFormat" })
@Html.TextBox("EndDate", DateTime.Now, htmlAttributes: new { @class = "form-control TextBoxFormat" })
@Html.DropDownList("ClientID", new SelectList(Model.ClientList, "ClientID", "ClientName"), "---Select---", new { @class = "dropdown" })
<input type="submit" value="Submit" />